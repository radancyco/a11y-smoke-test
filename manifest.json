---
layout: null
---
{
  "browser_action": {
    "browser_style": true,
    "default_icon": {
      "16": "img/icon-16.png",
      "32": "img/icon-32.png"
    },
    "default_popup": "index.html",
    "default_title": "{{ site.name }}"
  },
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com https://radancy.dev https://rawgit.com https://mraccess77.github.io https://www.ssa.gov https://khan.github.io; object-src 'self'",
  "description": "{{ site.description }}",
  "manifest_version": 2,
  "name": "{{ site.title }}",
  "permissions": [
    "activeTab"
  ],
  "version": "{{ site.version }}",
  "browser_specific_settings": {
    "gecko": {
      "id": "{ceae4868-0848-4301-b830-0074622a2a10}",
      "strict_min_version": "87.0",
      "update_url": "https://services1.tmpwebeng.com/browser-plugin/a11y-smoke-test/firefox/updates.json"
    }
  }
}
